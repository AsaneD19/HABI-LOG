<%= form_with(url: search_path, class: "form-inline mx-4", method: :get) do |f| %>
  <%= f.text_field :search, class: "form-control", placeholder: "search..." %>
  <%= button_tag type: 'submit', class: "btn btn-outline-success" do %>
    <i class="fa-solid fa-magnifying-glass"></i>
  <% end %>
<% end %>
