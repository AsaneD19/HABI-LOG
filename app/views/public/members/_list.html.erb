<div class="container">
  <div class="row">
    <div class="col-md-8 offset-md-2">
      <% members.each do |member| %>
        <%= link_to member_path(member), class: "text-decoration-none" do %>
          <%= render "public/members/data", member: member %>
        <% end %>
      <% end %>
    </div>
  </div>
</div>