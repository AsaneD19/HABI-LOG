<h1>登録メンバー一覧</h1>
<ul>
  <% @members.each do |member| %>
  <li>
    <%= link_to admin_member_path(member) do %>
      <%= member.name %>
    <% end %>
     - <%= member.account_id %> - <%= member.email %> -
    <%= link_to "削除", admin_member_path(member), method: :delete, data: { confirm: "#{member.account_id}を削除します。よろしいですか？" } %>
  </li>
  <% end %>
</ul>