<%= link_to "戻る", timeline_path %>

<h1><%= @member.nickname %></h1>
<div><%= @member.account_name %></div>

<% if @member.is_private == true %>
  <h1><%= 非公開メンバーです。 %></h1>
<% else %>
  <p>フォロー：<%= @member.follow_count %></p>
  <p>フォロワー：<%= @member.followed_count %></p>

  <% @feeds.each do |feed| %>
   <%= render "feeds/feed_data", feed: feed %>
  <% end %>
<% end %>